{ 
    "version": 2,
    "builds": [
        {
            "src" : "Server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "E-Commerce/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "E-Commerce/build"
            }
        }
    ],  
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "server/server.js" 
        }
    ] 
}
