{
    "version": 2,
      "functions": {
    "/server.js": {
      "maxDuration": 30  // Set timeout to 30 seconds (default is 10)
    }
  },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }
